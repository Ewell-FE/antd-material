{
  "root": "/material",
  "name": "antd-material",
  "main": "core/index.js",
  "version": "0.1.9",
  "authFlag": false,
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "dependencies": {
    "@devexpress/dx-react-core": "^1.0.3",
    "@devexpress/dx-react-grid": "^1.0.3",
    "@devexpress/dx-react-grid-material-ui": "^1.0.3",
    "@material-ui/core": "^1.0.0",
    "@material-ui/icons": "^1.0.0",
    "lodash.assign": "^4.2.0",
    "lodash.debounce": "^4.0.8",
    "malihu-custom-scrollbar-plugin": "^3.1.5",
    "moment": "^2.20.1",
    "nprogress": "^0.2.0",
    "object-assign": "4.1.1",
    "omit.js": "^1.0.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
    "raf": "3.4.0",
    "rc-calendar": "~9.6.0",
    "rc-cascader": "~0.13.0",
    "rc-editor-mention": "^1.0.2",
    "rc-input-number": "^4.0.0",
    "rc-menu": "^7.0.2",
    "rc-notification": "^3.1.1",
    "rc-progress": "^2.2.2",
    "rc-queue-anim": "^1.4.1",
    "rc-scroll-anim": "^2.2.1",
    "rc-select": "^8.1.0",
    "rc-slider": "^8.6.0",
    "rc-time-picker": "^3.3.0",
    "rc-tween-one": "^2.0.1",
    "rc-upload": "^2.4.0",
    "react": "^16.0.0",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.0.0",
    "react-highlight": "^0.12.0",
    "react-lazy-load": "^3.0.13",
    "react-native-web": "^0.1.16",
    "react-popper": "^0.10.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-scrollbar-size": "^2.1.0",
    "react-select": "^1.2.1",
    "react-slick": "^0.23.1",
    "react-swipeable-views": "^0.12.13",
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "redux-form-material-ui": "^5.0.0-beta.2",
    "redux-saga": "^0.16.0",
    "shallowequal": "^1.0.2",
    "slick-carousel": "^1.8.1"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "mock": "node scripts/startMock.js",
    "test": "node scripts/test.js --env=jsdom",
    "pub": "rimraf npm/core && rimraf npm/lib && node scripts/publish.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ],
    "ignore": [
      "./src/lib/**/*.js"
    ]
  },
  "proxy": "http://localhost:8000",
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "require-yield": 0,
      "no-undef": 0,
      "no-script-url": 0,
      "no-useless-escape": 0
    }
  },
  "eslintIgnore": [
    "src/lib/**"
  ]
}
