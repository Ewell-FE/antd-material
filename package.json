    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "expose-loader": "^0.7.4",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-clean-css": "^3.9.4",
    "gulp-export": "^2.2.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-rename": "^1.2.3",
    "gulp-uglify": "^3.0.0",
    "html-webpack-plugin": "2.29.0",
